<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="1222.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.lenden.SubscriptionInfoController">
   <bottom>
      <TilePane alignment="TOP_CENTER" orientation="VERTICAL" prefHeight="184.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="subscriptionStartDateLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="22.0" text="You have been with us since " textAlignment="CENTER">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
         </children>
      </TilePane>
   </bottom>
   <top>
      <TilePane alignment="BOTTOM_CENTER" orientation="VERTICAL" prefHeight="175.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="userLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="37.0" prefWidth="438.0" text="Hey User" textAlignment="CENTER">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
         </children>
      </TilePane>
   </top>
   <center>
      <TilePane alignment="BOTTOM_CENTER" orientation="VERTICAL" prefHeight="35.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="subscriptionEndDateLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="22.0" text="Your LenDen Subscription is valid till" textAlignment="CENTER">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
         </children>
      </TilePane>
   </center>
</BorderPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="previewWindow" scaleShape="false" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.lenden.BillPrintController">
   <children>
      <Label alignment="CENTER" prefHeight="42.0" prefWidth="280.0" text="Invoice" wrapText="true">
         <font>
            <Font name="Segoe UI Semibold" size="25.0" />
         </font>
      </Label>
      <Label fx:id="billLabel" contentDisplay="CENTER" text="Label" wrapText="true">
         <font>
            <Font name="Courier New" size="15.0" />
         </font>
      </Label>
      <Button fx:id="close" mnemonicParsing="false" onMouseClicked="#closePreviewWindow" prefHeight="34.0" prefWidth="109.0" text="Close">
         <VBox.margin>
            <Insets bottom="10.0" left="100.0" right="100.0" />
         </VBox.margin></Button>
      <Button fx:id="print" mnemonicParsing="false" onMouseClicked="#printBill" prefHeight="34.0" prefWidth="109.0" text="Print">
         <VBox.margin>
            <Insets left="100.0" right="100.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
